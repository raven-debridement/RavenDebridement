<launch>
  <include file="$(find prosilica_camera)/launch/generic.launch" />
  <node pkg="tf" type="static_transform_publisher" name="world_to_cam" 
    args="0 0 0.5 -1.57 0 -1.57 camera_optical_frame world 100" />
  <node ns="camera" pkg="image_proc" type="image_proc" name="image_proc"/>
  <node name="ar_pose" pkg="ar_pose" type="ar_multi" respawn="false"
    output="screen">
    <remap from="camera/image_raw" to="camera/image_rect" />
    <param name="marker_pattern_list" type="string"
      value="$(find ar_pose)/data/object_4x4"/>
    <param name="threshold" type="int" value="100"/>
  </node>

  <!-- Load reasonable defaults for the relative pose between cameras --> 
  <!-- <include file="$(find openni_launch)/launch/kinect_frames.launch"> 
    <arg name="camera" value="camera" /> 
  </include> -->
</launch>
